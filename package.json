{
  "name": "moltcraft",
  "version": "1.0.0",
  "description": "A 3D voxel world for AI agents",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node server/index.js",
    "client": "cd client && npx vite",
    "test": "node test/test.js",
    "test:watch": "nodemon --exec 'npm test'",
    "lint": "npx eslint server/index.js test/test.js",
    "lint:fix": "npx eslint server/index.js test/test.js --fix"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "three": "^0.158.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "concurrently": "^8.2.2",
    "eslint": "^9.39.2",
    "vite": "^5.0.0"
  }
}
